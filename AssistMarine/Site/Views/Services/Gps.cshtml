
@{
    ViewData["Title"] = "GPS Service";
}
@section metas
{
    <meta name="description" content="The location of your ship is as important to us as it is to you.">
    <meta name="keywords" content="gps">
}
<section id="breadcrumbs" class="breadcrumbs">
    <div class="container">

        <ol>
            <li><a asp-action="Services" asp-controller="Home" title="GPS">Services</a></li>
            <li>@ViewData["Title"]</li>
        </ol>

    </div>
</section><!-- End Breadcrumbs -->

<section id="portfolio" class="portfolio">
    <div class="container">

        <div class="section-title">
            <span>GPS</span>
            <h1>GPS</h1>

        </div>
        <div class="row portfolio-container" data-aos="fade-up" data-aos-delay="150">

            @foreach (var item in Model)
            {
                <div class="col-lg-4 col-md-6 portfolio-item filter-web">
                    <img src="~/img/gps/@item.Image" class="img-fluid" alt="" style="border: 1px solid rgba(255, 0, 0, .1)" ;>
                    <div class="portfolio-info">
                        <a href="~/img/gps/@item.Image" data-gall="portfolioGallery" class="venobox preview-link" title="GPS" alt="GPS"><i class="bx bx-expand"></i></a>
                        <h3 style="color:darkred; font-size:22px;"> @item.ProductName </h3>
                    </div>
                </div>
            }
        </div>
    </div>
</section>

